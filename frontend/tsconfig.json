{
  "compilerOptions": {
    "outDir": "./build",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es6",
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true
  }
}
